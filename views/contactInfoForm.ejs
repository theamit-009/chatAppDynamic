
<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <h1 class="text-center mb-3">
          <i class="fas fa-user-plus"></i> Contact Information
        </h1>
      
        <form action="/users/postUpdate" method="post">

          <div class="form-group">
              <label for="Name"> Name</label>
              <input type="text" value = "<%= objUser2.name %> " name= "name" class="form-control"  placeholder="Name"
              />
            </div>
            <div class="form-group">
              <label for="email">Email</label>
              <input type="email" value = "<%= objUser2.email %>" name="email" class="form-control"  placeholder="Email"
              />
            </div>
          <div class="form-group">
              <label for="phone">Phone</label>
              <input type="text" name="phone" value="<%= objUser2.phone %>" class="form-control"  placeholder="Phone"
              />
            </div>
            <div class="form-group">
                <label for="title">Title</label>
                <input type="text" name="title" value="<%= objUser2.title %>" class="form-control"  placeholder="Title"
                />
              </div> 
                      
      <center><button type="submit" class="btn btn-info">Submit</button> 
      </button></center>    
           
        </form>
      </div>
    </div>
  </div>
 